<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module>
    <inherits name="org.flixel.flixel_examples_core"/>
    <inherits name="org.flixel.flixel_html5"/>
    <add-linker name="xsiframe"/>
    <set-property name="user.agent" value="safari"/>
    <set-configuration-property name="devModeRedirectEnabled" value="true"/>
    <set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
    <entry-point class="org.flixel.client.FlxHtml5ExampleStarter"></entry-point>
    <set-configuration-property name="gdx.assetpath" value="../flixel-examples-android/assets"/>
    <extend-configuration-property name="gdx.reflect.include" value="org.flixel.examples.mode.MenuState"/>
    <extend-configuration-property name="gdx.reflect.include" value="org.flixel.examples.mode.PlayState"/>
    <extend-configuration-property name="gdx.reflect.include" value="org.flixel.examples.mode.Bullet"/>
    <extend-configuration-property name="gdx.reflect.include" value="org.flixel.examples.mode.EnemyBullet"/>
    <extend-configuration-property name="gdx.reflect.include" value="org.flixel.examples.mode.Enemy"/>
</module>